{
  "name": "vue-h-select",
  "version": "0.1.8",
  "description": "Smart select for Vue 2",
  "main": "index.vue",
  "keywords": [
    "vue",
    "select",
    "smart",
    "multiple",
    "tags"
  ],
  "scripts": {
    "lint": "npm run lint:js -s & npm run lint:css -s",
    "lint:js": "eslint --fix index.vue",
    "lint:css": "stylelint index.vue",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ]
  },
  "author": "Alexander Zonov",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-env": "1.6.1",
    "eslint": "4.17.0",
    "eslint-plugin-vue": "4.2.2",
    "jest": "22.3.0",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.0.0",
    "stylelint-processor-html": "1.0.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zont/vue-h-select"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
